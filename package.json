{
  "name": "esm-compiler",
  "version": "0.7.1",
  "description": "A tsx compiler for esm.sh playground powered by SWC.",
  "type": "module",
  "module": "./pkg/esm_compiler.js",
  "types": "./pkg/esm_compiler.d.ts",
  "exports": {
    ".": {
      "workerd": "./workerd/index.js",
      "import": "./pkg/esm_compiler.js",
      "types": "./pkg/esm_compiler.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "./types/swc.d.ts",
    "./pkg/esm_compiler_bg.wasm",
    "./pkg/esm_compiler.js",
    "./pkg/esm_compiler.d.ts",
    "./workerd/index.js"
  ],
  "scripts": {
    "prepublishOnly": "node build.mjs",
    "build": "node build.mjs",
    "test": "cargo test --all",
    "deploy": "node build.mjs && npx -y wrangler@3 deploy --name esm-compiler --compatibility-date 2024-08-01 workerd/worker.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-dev/esm-compiler.git"
  },
  "license": "MIT"
}
