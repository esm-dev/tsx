{
  "name": "esm-compiler",
  "version": "0.6.2",
  "description": "The compiler for esm.sh playground written in Rust.",
  "type": "module",
  "module": "./pkg/esm_compiler.js",
  "types": "./pkg/esm_compiler.d.ts",
  "exports": {
    ".": {
      "import": "./pkg/esm_compiler.js",
      "types": "./pkg/esm_compiler.d.ts"
    },
    "./swc": {
      "import": "./js/swc.js",
      "types": "./types/swc.d.ts"
    },
    "./lightningcss": {
      "import": "./js/lightningcss.js",
      "types": "./types/lightningcss.d.ts"
    },
    "./workerd": {
      "import": "./js/workerd.js",
      "types": "./types/workerd.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "./types/",
    "./js/",
    "./pkg/esm_compiler_bg.wasm",
    "./pkg/esm_compiler.js",
    "./pkg/esm_compiler.d.ts"
  ],
  "scripts": {
    "build": "wasm-pack build --target web",
    "test": "pnpm build && deno run -A js/test.js",
    "deploy": "pnpm build && npx -y wrangler@3 deploy --name esm-compiler --compatibility-date 2024-04-06 js/workerd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-dev/esm-compiler.git"
  },
  "license": "MIT"
}
